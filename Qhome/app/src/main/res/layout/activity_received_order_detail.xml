<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/activity"
    tools:context=".main.ReceivedOrderDetailActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:background="@drawable/toolbar_shadow">
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:padding="2dp"
                android:layout_marginLeft="10dp"
                android:layout_gravity="center_vertical"
                android:onClick="back"
                android:src="@drawable/leftarrow_marron"/>
            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textSize="22sp"
                android:textColor="@color/colorPrimary"
                android:text="@string/order"/>
        </FrameLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="0px"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="20dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/caption1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="14sp"
                                    android:text="@string/order_id"/>

                                <TextView
                                    android:id="@+id/caption2"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="14sp"
                                    android:layout_marginTop="15dp"
                                    android:text="@string/order_date"/>

                                <TextView
                                    android:id="@+id/caption10"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="14sp"
                                    android:layout_marginTop="15dp"
                                    android:text="@string/order_status"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="30dp"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/order_number"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="14sp"
                                    android:text="DD12hskdgdd234"/>

                                <TextView
                                    android:id="@+id/order_date"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="14sp"
                                    android:layout_marginTop="15dp"
                                    android:text="06/21/2019 12:50 PM"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/statusLegend"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="30dp"
                            android:layout_marginRight="30dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="11sp"
                                android:textColor="@color/green"
                                android:text="@string/placed"/>

                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="5dp"
                                android:layout_gravity="center_vertical"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="11sp"
                                android:textColor="@color/green"
                                android:text="@string/confirmed"/>

                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="5dp"
                                android:layout_gravity="center_vertical"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_vertical"
                                android:text="@string/prepared"
                                android:textColor="@color/green"
                                android:textSize="11sp" />

                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="5dp"
                                android:layout_gravity="center_vertical"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="11sp"
                                android:textColor="@color/green"
                                android:text="@string/ready"/>

                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="5dp"
                                android:layout_gravity="center_vertical"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center_vertical"
                                android:text="@string/delivered"
                                android:textColor="@color/green"
                                android:textSize="11sp" />

                        </LinearLayout>

                        <FrameLayout
                            android:id="@+id/trackFrame"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="12dp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:orientation="horizontal">

                                <View
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="5dp"
                                    android:layout_gravity="center_vertical" />

                                <ImageView
                                    android:id="@+id/img_placed"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/marroon_circle"/>

                                <View
                                    android:id="@+id/view_confirm"
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="5dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@color/gray"/>

                                <ImageView
                                    android:id="@+id/img_confirmed"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/gray_circle"/>

                                <View
                                    android:id="@+id/view_prepare"
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="5dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@color/gray"/>

                                <ImageView
                                    android:id="@+id/img_prepared"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/gray_circle"/>

                                <View
                                    android:id="@+id/view_ready"
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="5dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@color/gray"/>

                                <ImageView
                                    android:id="@+id/img_ready"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/gray_circle"/>

                                <View
                                    android:id="@+id/view_delivery"
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="5dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@color/gray"/>

                                <ImageView
                                    android:id="@+id/img_delivered"
                                    android:layout_width="16dp"
                                    android:layout_height="16dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/gray_circle"/>

                                <View
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="5dp"
                                    android:layout_gravity="center_vertical" />

                            </LinearLayout>

                        </FrameLayout>

                        <LinearLayout
                            android:id="@+id/buttonFrame"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="15dp"
                            android:padding="5dp"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/btn_cancel"
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="@color/dark_gray"
                                android:background="@drawable/gray_stroke"
                                android:onClick="cancelItem"
                                android:padding="5dp"
                                android:gravity="center"
                                android:textSize="14sp"
                                android:layout_marginLeft="30dp"
                                android:text="@string/cancel"/>

                            <TextView
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content" />

                            <TextView
                                android:id="@+id/btn_progress"
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textColor="#fff"
                                android:textSize="14sp"
                                android:background="@drawable/primary_rect_fill"
                                android:onClick="progressOrderItem"
                                android:padding="5dp"
                                android:gravity="center"
                                android:layout_marginRight="30dp"
                                android:text="@string/confirm"/>

                        </LinearLayout>

                        <TextView
                            android:id="@+id/caption11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_horizontal"
                            android:textSize="16sp"
                            android:padding="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginRight="8dp"
                            android:text="@string/order_detail"
                            android:textColor="#60830709"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="18dp"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/caption9"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_vertical"
                                    android:textSize="14sp"
                                    android:layout_marginRight="8dp"
                                    android:text="@string/contact"
                                    android:textColor="#60830709"/>
                                <View
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="2dp"
                                    android:layout_gravity="center_vertical"
                                    android:background="#EBEBEB"/>
                            </LinearLayout>

                            <TextView
                                android:id="@+id/phone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="2dp"
                                android:textSize="16sp"
                                android:paddingLeft="20dp"
                                android:paddingRight="10dp"
                                android:paddingTop="15dp"
                                android:paddingBottom="15dp"
                                android:hint="12345678902"
                                android:onClick="contact"
                                android:textColorHint="@color/colorPrimary"
                                android:drawableLeft="@drawable/ic_phone2"
                                android:drawablePadding="15dp"
                                android:drawableRight="@drawable/ic_right_marroon"
                                android:textColor="@color/colorPrimary"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="18dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption8"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="14sp"
                                android:layout_marginRight="8dp"
                                android:text="@string/shipping_address"
                                android:textColor="#60830709"/>
                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="2dp"
                                android:layout_gravity="center_vertical"
                                android:background="#EBEBEB"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="visible"
                            android:paddingBottom="15dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_marginLeft="18dp"
                                android:layout_marginRight="10dp"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/address"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="8dp"
                                    android:hint="Full Address"
                                    android:textColorHint="@color/colorPrimary"
                                    android:textSize="14sp"
                                    android:paddingRight="10dp"
                                    android:paddingTop="8dp"
                                    android:paddingBottom="8dp"
                                    android:drawableLeft="@drawable/ic_location"
                                    android:drawablePadding="15dp"
                                    android:textColor="@color/colorPrimary"/>

                                <TextView
                                    android:id="@+id/address2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:hint="Area, Street, House"
                                    android:textColorHint="@color/colorPrimary"
                                    android:textSize="12sp"
                                    android:paddingLeft="30dp"
                                    android:paddingRight="10dp"
                                    android:textColor="@color/colorPrimary"/>

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="18dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="14sp"
                                android:layout_marginRight="8dp"
                                android:text="@string/payment_method"
                                android:textColor="#60830709"/>
                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="2dp"
                                android:layout_gravity="center_vertical"
                                android:background="#EBEBEB"/>
                        </LinearLayout>

                        <TextView
                            android:id="@+id/method"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dp"
                            android:text="Cash"
                            android:textSize="16sp"
                            android:paddingLeft="18dp"
                            android:paddingRight="10dp"
                            android:paddingTop="15dp"
                            android:paddingBottom="15dp"
                            android:drawableLeft="@drawable/ic_cash2"
                            android:drawablePadding="15dp"
                            android:textColor="@color/colorPrimary"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="18dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption4"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:textSize="14sp"
                                android:layout_marginRight="8dp"
                                android:text="@string/items"
                                android:textColor="#60830709"/>
                            <View
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="2dp"
                                android:layout_gravity="center_vertical"
                                android:background="#EBEBEB"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0px"
                            android:layout_weight="1"
                            android:padding="8dp"
                            android:layout_marginBottom="20dp"
                            android:onClick="getProduct"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    android:layout_marginLeft="8dp"
                                    android:layout_marginTop="8dp"
                                    android:layout_marginRight="8dp">

                                    <com.makeramen.roundedimageview.RoundedImageView
                                        android:id="@+id/pictureBox"
                                        android:layout_width="85dp"
                                        android:layout_height="110dp"
                                        app:riv_corner_radius="8dp"
                                        android:src="@drawable/food"
                                        android:scaleType="centerCrop"/>

                                </LinearLayout>
                                <LinearLayout
                                    android:layout_width="0px"
                                    android:layout_weight="1"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:padding="5dp"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/productNameBox"
                                            android:layout_width="0px"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:textColor="@color/colorPrimary"
                                            android:textSize="14sp"
                                            android:text="Traditional Qatari Dishes"/>

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/storeNameBox"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textSize="14sp"
                                            android:textColor="@color/colorPrimary"
                                            android:layout_gravity="center_vertical"
                                            android:singleLine="true"
                                            android:ellipsize="end"
                                            android:padding="3dp"
                                            android:textStyle="bold"
                                            android:text="Carrefour"/>

                                        <TextView
                                            android:layout_width="0px"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content" />

                                        <TextView
                                            android:id="@+id/categoryBox"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textSize="14sp"
                                            android:textColor="@color/colorPrimary"
                                            android:layout_gravity="center_vertical"
                                            android:layout_marginRight="10dp"
                                            android:singleLine="true"
                                            android:ellipsize="end"
                                            android:padding="3dp"
                                            android:textStyle="italic"
                                            android:text="Food"/>

                                    </LinearLayout>

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="0px"
                                        android:layout_weight="1"/>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/quantityBox"
                                            android:layout_width="0px"
                                            android:layout_weight="1"
                                            android:layout_height="wrap_content"
                                            android:textSize="14sp"
                                            android:singleLine="true"
                                            android:paddingLeft="8dp"
                                            android:textColor="@color/colorPrimary"
                                            android:text="X 1"/>

                                        <TextView
                                            android:id="@+id/statusBox"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:textSize="14sp"
                                            android:singleLine="true"
                                            android:layout_marginRight="10dp"
                                            android:layout_gravity="center_vertical"
                                            android:textColor="@color/green"
                                            android:text="@string/placed"/>

                                    </LinearLayout>

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </ScrollView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@drawable/white_vertical_gradient"
                android:orientation="vertical">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_marginLeft="8dp"
                    android:layout_marginRight="8dp"
                    android:paddingTop="5dp"
                    android:background="#EBEBEB"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingLeft="10dp"
                    android:paddingTop="15dp"
                    android:paddingBottom="20dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="0px"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/subtotal"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:textColor="@color/colorPrimary"/>
                            <TextView
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content" />
                            <TextView
                                android:id="@+id/subtotal_price"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="14.5USD"
                                android:textSize="14sp"
                                android:textColor="@color/colorPrimary"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/bonusFrame"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:visibility="gone"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption12"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/bonus"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:textColor="@color/colorPrimary"/>
                            <TextView
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content" />
                            <TextView
                                android:id="@+id/bonus"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="1USD"
                                android:textSize="14sp"
                                android:textColor="@color/colorPrimary"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/shipping"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:textColor="@color/colorPrimary"/>
                            <TextView
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content" />
                            <TextView
                                android:id="@+id/shipping_price"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="1USD"
                                android:textSize="14sp"
                                android:textColor="@color/colorPrimary"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/caption7"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/total"
                                android:textSize="16sp"
                                android:textStyle="bold"
                                android:gravity="center"
                                android:textColor="@color/colorPrimary"/>
                            <TextView
                                android:layout_width="0px"
                                android:layout_weight="1"
                                android:layout_height="wrap_content" />
                            <TextView
                                android:id="@+id/total_price"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginRight="10dp"
                                android:text="15.5USD"
                                android:textSize="14sp"
                                android:textColor="@color/colorPrimary"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

    <com.wang.avi.AVLoadingIndicatorView
        android:id="@+id/loading_bar"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:visibility="gone"
        app:indicator="LineScaleParty"
        app:indicator_color="#F53700" />

</FrameLayout>



























